#content
    margin-top 30px
    border 1px solid #eee
    border-radius 3px
    background white 
    box-shadow 0px 2px 5px #ccc 
    padding 0px 
    overflow hidden

#header
    background #E03137 
    color white 
    margin 0px 
    height 90px
    line-height 90px

#header h1 
    display block
    line-height 90px 
    font-weight bold 
    font-size 30px 



#nav 
    padding 0px
    margin 
    border 1px solid #eee 
    box-shadow 10px 2px 5px #ccc 
    background #FF636E

 
#nav ul 
    margin 0px
    list-style none 
    padding-left 10px

#nav ul li
    float left
    margin-right 30px 

#nav ul li a
    display block
    color white
    height 30px 
    line-height 30px
    padding-left 10px
    padding-right 10px
    transition all 300ms

#nav ul li a:hover
    text-decoration none 
    background #E03137

#main .jumbotron
    padding 50px
    margin-top 30px 
    border-radius 20px

#main .jumbotron a
    margin 5px

#footer
    margin-top 50px 
    padding-top 5px
    border-top 3px solid #eee 
    color gray 

#footer #subfooter a
    color: #555051